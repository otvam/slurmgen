{
    "tag": "test",
    "control": {
        "overwrite": true,
        "sbatch": true,
        "folder": "slurm_output"
    },
    "env": {
        "folder_delete": ["data_input", "data_output"],
        "folder_create": ["data_input", "data_output"],
        "var": {
            "PYTHONUNBUFFERED": "1",
            "CONFIGHPC": "1"
        },
        "conda": {
            "path": "conda.sh",
            "name": "envname"
        }
    },
    "job": {
        "pragmas": {
            "time":  "23:00:00",
            "nodes":  "1",
            "ntasks-per-node": "16",
            "mem":  "64G"
        },
        "commands": [
            {
                "tag": "python",
                "executable": "python",
                "arguments": ["run_solve.py", "arg_1", "arg_2"]
            },
            {
                "tag": "perl",
                "executable": "perl",
                "arguments": ["run_solve.pl", "arg_1", "arg_2"]
            }
        ]
    }
}
