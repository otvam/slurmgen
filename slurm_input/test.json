{
    "tag": "test",
    "control": {
        "overwrite": true,
        "sbatch": true,
        "folder": "slurm_output"
    },
    "env": {
        "folder_delete": ["data_input", "data_output"],
        "folder_create": ["data_input", "data_output"],
        "var": {
            "PYTHONUNBUFFERED": "1",
            "CONFIGHPC": "1"
        },
        "conda": {
            "path": "conda.sh",
            "name": "envname"
        }
    },
    "job": {
        "resources": {
            "time":  "23:00:00",
            "nb_nodes":  "1",
            "nb_tasks": "16",
            "memory":  "64G",
            "list_nodes":  null
        },
        "command": {
            "exe": "python",
            "script": "run_solve.py",
            "arg_list": ["arg_1", "arg_2"]
        }
    }
}
